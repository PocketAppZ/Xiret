Friend Class LogColor

    Friend Shared Function Xiret() As Color
        Return Color.FromArgb(255, 51, 255)
    End Function
    Friend Shared Function Winsat() As Color
        Return Color.FromArgb(252, 209, 40)
    End Function
    Friend Shared Function Information() As Color
        Return Color.FromArgb(26, 209, 255)
    End Function
    Friend Shared Function Okay() As Color
        Return Color.FromArgb(0, 255, 0)
    End Function
    Friend Shared Function Warning() As Color
        Return Color.FromArgb(255, 133, 50)
    End Function
    Friend Shared Function ErrorRed() As Color
        Return Color.FromArgb(255, 20, 50)
    End Function

End Class
